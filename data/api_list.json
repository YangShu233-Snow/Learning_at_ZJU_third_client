                    "no-intercept": "true"
                }
            }
        }
    },
    "resources_submission": {
        "apis_name": [
            "upload",
            "submissions"
        ],
        "apis_config": {
            "upload": {
                "url": "https://courses.zju.edu.cn/api/uploads",
                "method": "POST",
                "data": {
                    "name": "Default_name",
                    "size": 1,
                    "parent_type": null,
                    "parent_id": 0,
                    "is_scorm": false,
                    "is_wmpkg": false,
                    "source": "",
                    "is_marked_attachment": false,
                    "embed_material_type": ""
                }
            },
            "submissions": {
                "url": "https://courses.zju.edu.cn/api/course/activities",
                "method": "POST",
                "data": {
                    "comment": "<p><span style=\"font-size: 14px;\">{{{comment}}}</span><br></p>",
                    "uploads": [],
                    "slides": "",
                    "is_draft": false,
                    "mode": "normal",
                    "other_resources": [],
                    "uploads_in_rich_text": []
                }
            }
        }
    },
    "course_resource_download": {
        "apis_name": [
            "download"
        ],
        "apis_config": {
            "download": {
                "url": "https://courses.zju.edu.cn/api/uploads/reference/<placeholder>/blob",
                "params": {}
            }
        }
    },
    "resource": {
        "apis_name": [
            "list",
            "download",
            "remove",
            "batch_remove"
        ],
        "apis_config": {
            "list": {
                "url": "https://courses.zju.edu.cn/api/user/resources",
                "params": {
                    "conditions": {
                        "keyword": "",
                        "includeSlides": true,
                        "limitTypes": [],
                        "fileType": "all",
                        "parentId": 0,
                        "folderToken": "",
                        "resourceType": null,
                        "filters": [],
                        "linkTypes": [],
                        "only_ready": false
                    },
                    "page": 1,
                    "page_size": 10
                }
            },
            "download": {
                "url": "https://courses.zju.edu.cn/api/uploads/<placeholder>/blob",
                "params": {}
            },
            "remove": {
                "url": "https://courses.zju.edu.cn/api/user/upload/<placeholder>",
                "method": "DELETE",
                "params": {}
            },
            "batch_remove": {
                "url": "https://courses.zju.edu.cn/api/user/uploads",
                "method": "DELETE",
                "params": {
                    "upload_ids": []
                }
            }
        }
    }
