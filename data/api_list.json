{
    "course": {
        "apis_name": [
            "list",
            "view",
            "modules",
            "activities",
            "exams",
            "exam-scores",
            "completeness",
            "classrooms",
            "activities_reads",
            "coursewares",
            "homework-completeness",
            "exam-completeness",
            "enrollments"
        ],
        "apis_config": {
            "list": {
                "url": "https://courses.zju.edu.cn/api/my-courses",
                "method": "GET",
                "params": {
                    "conditions": {
                        "status": [
                            "ongoing",
                            "notStarted"
                        ],
                        "keyword": "",
                        "classify_type": "recently_started",
                        "display_studio_list": false
                    },
                    "fileds": "id,name,course_code,academic_year_id,course_attributes(teaching_class_name),course_code,department(id,name),instructors(id,name),start_date,url",
                    "page": 1,
                    "page_size": 10,
                    "showScorePassedStatus": "false"
                }
            },
            "view": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>",
                "method": "GET",
                "params": {
                    "fileds": "name"
                }
            },
            "modules": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/modules",
                "method": "GET",
                "params": {}
            },
            "activities": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/activities",
                "method": "GET",
                "params": {
                    "sub_course_id": 0
                }
            },
            "exams": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/exams",
                "method": "GET",
                "params": {
                    "no-intercept": "true"
                }
            },
            "exam-scores": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/exam-scores",
                "method": "GET",
                "params": {
                    "no-intercept": "true"
                }
            },
            "completeness": {
                "url": "https://courses.zju.edu.cn/api/course/<placeholder>/my-completeness",
                "method": "GET",
                "params": {}
            },
            "classrooms": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/classroom-list",
                "method": "GET",
                "params": {}
            },
            "activities_reads": {
                "url": "https://courses.zju.edu.cn/api/course/<placeholder>/activity-reads-for-user",
                "method": "GET",
                "params": {}
            },
            "coursewares": {
                "url": "https://courses.zju.edu.cn/api/course/<placeholder>/coursewares",
                "method": "GET",
                "params": {
                    "conditions": {
                        "category": null,
                        "itemsSortBy": {
                            "predicate": "chapter",
                            "reverse": false
                        },
                        "ignore_activity_types": [
                            "lesson"
                        ]
                    },
                    "page": 1,
                    "page_size": 20
                }
            },
            "homework-completeness": {
                "url": "https://courses.zju.edu.cn/api/course/<placeholder>/homework/submission-status",
                "method": "GET",
                "params": {
                    "no-intercept": true
                }
            },
            "exam-completeness": {
                "url": "https://courses.zju.edu.cn/api/courses/<placeholder>/submitted-exams",
                "method": "GET",
                "params": {
                    "no-intercept": true
                }
            },
            "enrollments": {
                "url": "https://courses.zju.edu.cn/api/course/<placeholder>/enrollments",
                "method": "GET",
                "params": {
                    "conditions": "",
                    "fields": "id,user(id,name),roles"
                }
            }
        }
    },
    "resource": {
        "apis_name": [
            "list",
            "download",
            "batch_download",
            "remove",
            "batch_remove",
            "upload"
        ],
        "apis_config": {
            "list": {
                "url": "https://courses.zju.edu.cn/api/user/resources",
                "method": "GET",
                "params": {
                    "conditions": {
                        "keyword": "",
                        "includeSlides": true,
                        "limitTypes": [],
                        "fileType": "all",
                        "parentId": 0,
                        "folderToken": "",
                        "resourceType": null,
                        "filters": [],
                        "linkTypes": [],
                        "only_ready": false
                    },
                    "page": 1,
                    "page_size": 10
                }
            },
            "download": {
                "url": "https://courses.zju.edu.cn/api/uploads/<placeholder>/blob",
                "method": "GET",
                "params": {}
            },
            "batch_download": {
                "url": "https://courses.zju.edu.cn/zip/uploads",
                "method": "GET",
                "params": {
                    "timezone": -480,
                    "upload_ids": []
                }
            },
            "remove": {
                "url": "https://courses.zju.edu.cn/api/user/upload/<placeholder>",
                "method": "DELETE",
                "params": {}
            },
            "batch_remove": {
                "url": "https://courses.zju.edu.cn/api/user/uploads",
                "method": "DELETE",
                "params": {
                    "upload_ids": []
                }
            },
            "upload": {
                "url": "https://courses.zju.edu.cn/api/uploads",
                "method": "POST",
                "data": {
                    "name": "Default_name",
                    "size": 1,
                    "parent_type": null,
                    "parent_id": 0,
                    "is_scorm": false,
                    "is_wmpkg": false,
                    "source": "",
                    "is_marked_attachment": false,
                    "embed_material_type": ""
                }
            }
        }
    },
