            "activities": {
                "url": "https://courses.zju.edu.cn/api/courses",
                "params": {}
            },
            "exams": {
                "url": "https://courses.zju.edu.cn/api/courses",
                "params": {}
            },
            "homework": {
                "url": "https://courses.zju.edu.cn/api/course",
                "params": {
                    "no-intercept": "true"
                }
            },
            "submitted-exams": {
                "url": "https://courses.zju.edu.cn/api/courses",
